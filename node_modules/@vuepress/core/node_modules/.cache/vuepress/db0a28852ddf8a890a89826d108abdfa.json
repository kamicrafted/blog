uchEnd: function onTouchEnd(e) {\n      var dx = e.changedTouches[0].clientX - this.touchStart.x;\n      var dy = e.changedTouches[0].clientY - this.touchStart.y;\n\n      if (Math.abs(dx) > Math.abs(dy) && Math.abs(dx) > 40) {\n        if (dx > 0 && this.touchStart.x <= 80) {\n          this.toggleSidebar(true);\n        } else {\n          this.toggleSidebar(false);\n        }\n      }\n    },\n    onSWUpdated: function onSWUpdated(e) {\n      this.swUpdateEvent = e;\n    }\n  }\n};",{"version":3,"sources":["Layout.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgEA,OAAA,GAAA,MAAA,KAAA;AACA,OAAA,SAAA,MAAA,WAAA;AACA,OAAA,IAAA,MAAA,mBAAA;AACA,OAAA,IAAA,MAAA,mBAAA;AACA,OAAA,IAAA,MAAA,mBAAA;AACA,OAAA,MAAA,MAAA,yBAAA;AACA,OAAA,OAAA,MAAA,0BAAA;AACA,OAAA,aAAA,MAAA,gCAAA;AACA,SAAA,mBAAA,QAAA,QAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,IAAA,EAAA,IADA;AAEA,IAAA,IAAA,EAAA,IAFA;AAGA,IAAA,OAAA,EAAA,OAHA;AAIA,IAAA,MAAA,EAAA,MAJA;AAKA,IAAA,aAAA,EAAA;AALA,GADA;AASA,EAAA,IATA,kBASA;AACA,WAAA;AACA,MAAA,aAAA,EAAA,KADA;AAEA,MAAA,aAAA,EAAA;AAFA,KAAA;AAIA,GAdA;AAgBA,EAAA,QAAA,EAAA;AACA,IAAA,gBADA,8BACA;AAAA,UACA,WADA,GACA,KAAA,KADA,CACA,WADA;AAAA,UAEA,WAFA,GAEA,KAAA,KAFA,CAEA,WAFA;;AAGA,UACA,WAAA,CAAA,MAAA,KAAA,KAAA,IACA,WAAA,CAAA,MAAA,KAAA,KAFA,EAEA;AACA,eAAA,KAAA;AACA;;AACA,aACA,KAAA,MAAA,IACA,WAAA,CAAA,IADA,IAEA,WAAA,CAAA,IAFA,IAGA,WAAA,CAAA,GAHA,IAIA,KAAA,kBAAA,CAAA,GALA;AAOA,KAhBA;AAkBA,IAAA,iBAlBA,+BAkBA;AAAA,UACA,WADA,GACA,KAAA,KADA,CACA,WADA;AAEA,aACA,CAAA,WAAA,CAAA,MAAA,IACA,CAAA,WAAA,CAAA,IADA,IAEA,WAAA,CAAA,OAAA,KAAA,KAFA,IAGA,KAAA,YAAA,CAAA,MAJA;AAMA,KA1BA;AA4BA,IAAA,YA5BA,0BA4BA;AACA,aAAA,mBAAA,CACA,KAAA,KADA,EAEA,KAAA,MAFA,EAGA,KAAA,KAHA,EAIA,KAAA,WAJA,CAAA;AAMA,KAnCA;AAqCA,IAAA,WArCA,yBAqCA;AACA,UAAA,aAAA,GAAA,KAAA,KAAA,CAAA,WAAA,CAAA,SAAA;AACA,aAAA,CACA;AACA,qBAAA,CAAA,KAAA,gBADA;AAEA,wBAAA,KAAA,aAFA;AAGA,sBAAA,CAAA,KAAA;AAHA,OADA,EAMA,aANA,CAAA;AAQA;AA/CA,GAhBA;AAkEA,EAAA,OAlEA,qBAkEA;AAAA;;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,